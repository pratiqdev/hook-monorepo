{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {},
    "dev": {
      "outputs": ["dist/**", ".next/**"],
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "clean": {
      "cache": false
    }
  }
}
