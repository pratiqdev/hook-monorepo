{
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".js$",
        "excludePattern": "{node_modules/|Documentation}"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./src/Application/NodeJS-App/Resources/Documentation",
        "template": "./jsDocTemplate"
    }
}