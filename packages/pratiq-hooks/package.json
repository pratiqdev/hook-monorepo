{
  "name": "@pratiq/hooks",
  "version": "1.1.11",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/esm/**",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsup",
    "dev": "tsup",
    "//_dev": "tsup src/index.ts --format esm --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test-node": "node tests/node/test_1",
    "test-unit": "mocha"
  },
  "dependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@pratiq/utils": "workspace:*",
    "chai": "^4.3.7",
    "mocha": "^10.2.0"
  },
  "devDependencies": {
    "@pratiq/config": "workspace:*",
    "@types/debug": "^4.1.7",
    "@types/node": "^20.4.9",
    "@types/react": "^17.0.13",
    "debug": "^4.3.4",
    "eslint": "^7.32.0",
    "react": "^17.0.2",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "entry": [
      "./src/index.ts",
      "./src/hooks/**/*.tsx",
      "./src/utils/**/*.js",
      "./src/utils/**/*.ts"
    ],
    "format": [
      "esm"
    ],
    "splitting": true,
    "sourcemap": false,
    "clean": true,
    "shims": false,
    "target": "es2018",
    "bundle": false,
    "dts": true,
    "replaceNodeEnv": false,
    "minify": false,
    "minifyWhitespace": false,
    "minifyIdentifiers": false,
    "minifySyntax": false,
    "legacyOutput": false
  }
}
